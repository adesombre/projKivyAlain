#:import widget widget
#:import layout layout
#:import navigation_screen_manager navigation_screen_manager




MyScreenManager:

<MyScreenManager@NavigationScreenManager>:
    Screen:
        name:"screen1"
        BoxLayout:
            Button:
                text:"aller vers écran images"
                on_press:root.push("ImagesExemple")
            Button:
                text:"aller vers écran 3"
                on_press:root.push("screen3")
    Screen:
        name: "ImagesExemple"
        BoxLayout:
            orientation: "vertical"
            ActionBar:
                ActionView:
                    ActionPrevious:
                        title:"Images"
                        on_release: root.pop()
                        app_icon:"images/lab_logo.png"
                        app_icon_height:"28dp"
                        app_icon_width: "28dp"
            ImagesExemple:
    Screen:
        name:"screen2"
        Button:
            text:"Button2"
            on_press:root.pop()
    Screen:
        name:"screen3"
        Button:
            text:"Button3"
            on_press:root.pop()

